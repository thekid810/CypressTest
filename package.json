{
  "name": "practicetest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "runTestcaseInLocalInFirefox": "npx cypress run --browser firefox --headless --spec cypress/e2e/PracticeTest/**/*.feature",
    "runTestcaseInLocalInChrome": "npx cypress run --browser chrome --headless --spec cypress/e2e/PracticeTest/**/*.feature",
    "runTestcaseInLocalInEdge": "npx cypress run --browser edge --headless --spec cypress/e2e/PracticeTest/**/*.feature",
    "generateReport": "node cucumber-html-report.js",
    "openReportInMacOS": "open cypress/reports/cucumberReport/index.html",
    "scenarioInFirefox": "run-s -c runTestcaseInLocalInFirefox generateReport",
    "scenarioInChrome": "run-s -c runTestcaseInLocalInChrome generateReport",
    "scenarioInEdge": "run-s -c runTestcaseInLocalInEdge generateReport"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/reports/report.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/reports/cypressReport/report.html"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.3.0",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.1",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "multiple-cucumber-html-reporter": "^3.1.0",
    "npm-run-all": "^4.1.5"
  }
}
